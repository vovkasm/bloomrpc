{
  "name": "bloomrpc",
  "productName": "BloomRPC",
  "version": "1.5.4",
  "description": "GRPC GUI client",
  "main": "dist-electron/main/index.js",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build && electron-builder -c electron-builder.config.ts",
    "format": "prettier . -w",
    "postinstall": "patch-package"
  },
  "debug": {
    "env": {
      "VITE_DEV_SERVER_URL": "http://127.0.0.1:7777/"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vovkasm/bloomrpc.git"
  },
  "author": {
    "name": "Fabrizio Fenoglio (UtilityWarehouse)"
  },
  "license": "LGPL-3.0-or-later",
  "bugs": {
    "url": "https://github.com/vovkasm/bloomrpc/issues"
  },
  "keywords": [
    "gRPC",
    "GUI",
    "bloomRPC",
    "BloomRPC",
    "gRPC GUI"
  ],
  "homepage": "https://github.com/vovkasm/bloomrpc",
  "devDependencies": {
    "@blueprintjs/core": "^5.3.0",
    "@grpc/grpc-js": "^1.9.2",
    "@grpc/proto-loader": "^0.7.9",
    "@trivago/prettier-plugin-sort-imports": "^4.2.0",
    "@types/mousetrap": "^1.6.11",
    "@types/node": "^18.0.0",
    "@types/react": "^17.0.65",
    "@types/react-dom": "^17.0.20",
    "@types/react-test-renderer": "^17.0.3",
    "@types/uuid": "^9.0.3",
    "@types/validator": "^13.1.3",
    "@vitejs/plugin-react": "^4.0.4",
    "ace-builds": "^1.24.1",
    "cross-spawn": "^7.0.3",
    "electron": "26.2.1",
    "electron-builder": "^24.6.4",
    "electron-store": "^8.1.0",
    "grpc-web": "^1.4.2",
    "identity-obj-proxy": "^3.0.0",
    "lodash.get": "^4.4.2",
    "mobx": "^6.9.1",
    "mobx-react-lite": "3.4.3",
    "mobx-utils": "^6.0.7",
    "patch-package": "^8.0.0",
    "prettier": "^3.0.3",
    "protobufjs": "^7.2.5",
    "re-resizable": "^6.9.11",
    "react": "^17.0.2",
    "react-ace": "^10.1.0",
    "react-dom": "^17.0.2",
    "react-hot-loader": "^4.13.1",
    "react-sortable-hoc": "^2.0.0",
    "react-test-renderer": "^17.0.2",
    "sass": "^1.67.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6",
    "uuid": "^9.0.0",
    "validator": "^13.5.2",
    "vite": "^4.4.9",
    "vite-plugin-electron": "^0.14.0",
    "vite-plugin-electron-renderer": "^0.14.5"
  },
  "devEngines": {
    "node": ">=8.12.x",
    "npm": ">=4.x",
    "yarn": ">=0.21.3"
  }
}
